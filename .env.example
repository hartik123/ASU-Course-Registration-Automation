# Copy this file to .env and fill in your real values.
# .env is gitignored and will never be committed.

# Gmail account used to send notification emails
EMAIL_SENDER=your_gmail@gmail.com

# Gmail App Password (NOT your regular Gmail password).
# Generate one at: https://myaccount.google.com/apppasswords
EMAIL_APP_PASSWORD=your_gmail_app_password_here

# Where notification emails are sent
EMAIL_RECEIVER=your_asu_id@asu.edu

# ASU login credentials (webapp4.asu.edu)
ASU_USERNAME=your_asurite_id
ASU_PASSWORD=your_asu_password
